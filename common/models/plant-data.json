{
  "name": "PlantData",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "espId": {
      "type": "string",
      "required": true
    },
    "soil_moisture": {
      "type": "number"
    },
    "humidity": {
      "type": "number"
    },
    "temperature": {
      "type": "number"
    },
    "watertank": {
      "type": "number"
    },
    "water": {
      "type": "boolean"
    },
    "measuring_time": {
      "type": "date",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "plants": {
      "type": "belongsTo",
      "model": "Plants",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
